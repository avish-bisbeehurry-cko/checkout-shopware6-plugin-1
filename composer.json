{
  "name": "checkoutcom/shopware6",
  "description": "Checkout.com Shopware6 plugin",
  "version": "1.0.19",
  "type": "shopware-platform-plugin",
  "license": "MIT",
  "require": {
    "phpunit/phpunit": "^8 || ^9",
    "shopware/core": "*",
    "monolog/monolog": "^1 || ^2"
  },
  "autoload": {
    "psr-4": {
      "Checkoutcom\\": "src/"
    }
  },
  "extra": {
    "shopware-plugin-class": "Checkoutcom\\Checkoutcom",
    "plugin-icon": "src/Resources/public/cko-favicon.ico",
    "copyright": "(c) by checkout",
    "label": {
      "de-DE": "Checkout.com",
      "en-GB": "Checkout.com"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12.84",
    "paragonie/sodium_compat": "^1.8",
  "shopware/storefront": "*",
    "shopware/administration": "*"
  }
}